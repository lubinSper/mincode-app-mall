.dialog_warp {
    position: fixed;
    top: 50%;
    width: 100%;
    margin-top: -34rpx;
    z-index: 999999999999999999999999;
    text-align: center;
}

.dialog {
    background: rgba(0,0,0,0.8);
    line-height: 40rpx;
    text-align: center;
    border-radius: 8rpx;
    display: inline-block;
    padding: 14rpx 30rpx;
    color: #fff;
    font-size: 30rpx;
}

view {
    padding: 0;
    margin: 0;
}

.clear {
    clear: both;
    height: 0;
}

ul,li {
    margin: 0;
    padding: 0;
}

.plColor {
    color: #aaa;
    font-size: 28rpx;
}

.order_sure_box {
    width: 100%;
    overflow-x: hidden;
}

.order_product_list {
    width: 100%;
    margin: 0 auto;
    background-color: #f8f8f8;
}

.order_product_list .order_product_list_ul {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}

.order_product_list .order_product_list_li {
    position: relative;
    width: 100%;
    margin: 0 auto;
    border-bottom: 10rpx solid #fff;
    background-color: #f8f8f8;
    padding: 20rpx 0;
    height: 150rpx;
}

.order_product_list .order_product_list_li:last-child {
    border-bottom: 0rpx solid #f8f8f8;
}

.order_product_list .order_product_list_li dl {
    float: left;
    width: 100%;
}

.order_product_list .order_product_list_li dl dt image {
    width: 150rpx;
    height: 150rpx;
    display: block;
    background-color: #eee;
}

.order_product_list .order_product_list_li dl dt {
    margin-left: 20rpx;
}

.order_product_list .order_product_list_li dl dt,.order_product_list .order_product_list_li dl dd {
    float: left;
}

.order_product_list .order_product_list_li dl .opll_mid {
    margin-left: 20rpx;
    max-width: 380rpx;
    display: table-row;
    vertical-align: top;
}

.order_product_list .order_product_list_li dl .opll_mid .opll_mid_title {
    max-height: 64rpx;
    line-height: 32rpx;
    overflow: hidden;
    width: 100%;
    font-size: 26rpx;
    color: #222;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-wrap: break-word;
    word-break: break-all;
}

.order_product_list .order_product_list_li dl .opll_mid .opll_mid_price {
    font-size: 26rpx;
    height: 50rpx;
    line-height: 32rpx;
    width: 100%;
    margin-top: 10rpx;
}

.order_product_list .order_product_list_li dl .opll_mid .opll_mid_price ABCD {
    color: #888;
    float: left;
    margin-right: 20rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
}

.order_product_list .order_product_list_li dl .opll_ri {
    float: right;
    line-height: 38rpx;
    font-size: 26rpx;
    text-align: right;
    margin-right: 20rpx;
}

.order_product_list .order_product_list_li dl .opll_ri .opll_ri_grey {
    color: #888;
}

.order_product_list .order_product_list_li dl .opll_ri .opll_originalPrice {
    text-decoration: line-through;
}

.pull {
    width: 100%;
    display: inline-block;
    margin: 0 auto;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 26rpx;
    text-align: center;
    color: #555;
}

.pull abcd {
    padding-right: 32rpx;
}

.open_retract {
    background-color: #fff;
    margin-top: -12rpx;
    border-bottom: 1rpx solid #e2e2e2;
}

.retract abcd {
    background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/one-shop-one-mini-app/up.png) no-repeat right 11rpx;
    background-size: 22rpx 12rpx;
}

.pull_open abcd {
    background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/one-shop-one-mini-app/down.png) no-repeat right 13rpx;
    background-size: 22rpx 12rpx;
}

.leaving_message {
    background-color: #fff;
    position: relative;
    height: 70rpx;
    padding-top: 20rpx;
}

.leaving_message .button {
    display: block;
    width: 100%;
    height: 40rpx;
    background: none;
    margin: 0;
    padding: 0;
}

.leaving_message .button::after {
    border: none;
}

.leaving_message input {
    margin: 0 30rpx;
    width: 690rpx;
    line-height: 40rpx;
    height: 40rpx;
    color: #555;
    font-size: 28rpx;
    text-align: left;
}

.distribution_mode {
    width: 100%;
    display: inline-block;
    margin-top: 20rpx;
    font-size: 28rpx;
}

.distribution_mode .distribution_mode_tab {
    width: 100%;
