    text-align: center;
}

.distribution_mode .distribution_mode_tab .form {
    width: auto;
}

.distribution_mode .distribution_mode_tab .form .button {
    display: inline;
    background: none;
    margin: 0;
    padding: 0;
}

.distribution_mode .distribution_mode_tab .form .button::after {
    border: none;
}

.distribution_mode .distribution_mode_tab view text {
    display: inline-block;
    height: 88rpx;
    line-height: 88rpx;
    padding-bottom: 0rpx;
    color: #222;
    font-size: 28rpx;
}

.distribution_mode .distribution_mode_tab view .current {
    border-bottom: 4rpx solid #ff7800;
    color: #ff7800;
}

.dmc_content_infor {
    background-color: #fff;
    color: #222222;
}

.dmc_content_infor ul li {
