page {
    background-color: #dfb741;
}

.support_content {
    margin: 40rpx 30rpx;
    height: 920rpx;
    padding-top: 90rpx;
    box-sizing: border-box;
    text-align: center;
    background-color: #fff;
    border-radius: 16rpx;
    position: relative;
}

.support_head {
    margin: 0 auto;
    width: 220rpx;
    height: 220rpx;
}

.support_head image {
    width: 220rpx;
    height: 220rpx;
}

.support_desc {
    width: 506rpx;
    font-size: 30rpx;
    color: #888888;
    margin: 50rpx auto 20rpx auto;
}

.tel_number {
    width: 328rpx;
    height: 46rpx;
    line-height: 46rpx;
    font-size: 46rpx;
    margin: 0 auto;
}

.tel_chip2,.tel_chip3 {
    margin-left: 20rpx;
}

.support_tel {
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    background-color: #ff7800;
    margin: 50rpx auto 0 auto;
    color: #fff;
    font-size: 32rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.support_tel image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;
}

.support_intro {
    background-color: #fff5ee;
    height: 210rpx;
    width: 100%;
    color: #ff7800;
    border-radius: 16rpx;
    position: absolute;
    left: 0;
    bottom: 0;
    padding-top: 20rpx;
}

.intro_title {
    font-size: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: 54rpx;
}

.intro_link {
    width: 200rpx;
    height: 60rpx;
    font-size: 30rpx;
    line-height: 60rpx;
    border: 2rpx solid #ff7800;
    margin: 20rpx auto 0 auto;
}

.support_bg {
    position: absolute;
    left: 0;
    bottom: 210rpx;
    width: 100%;
    height: 20rpx;
    z-index: 9;
    background-color: #fff;
}

.version-num {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    color: #826307;
    font-size: 26rpx;
    text-align: center;
    position: absolute;
    left: 0;
    bottom: 60rpx;
}