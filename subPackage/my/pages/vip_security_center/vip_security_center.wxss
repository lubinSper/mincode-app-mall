page {
    overflow: hidden;
}

.menu_bar {
    background-color: #fff;
    padding: 20rpx 30rpx 20rpx 40rpx;
    padding-right: 0;
    margin-top: 20rpx;
}

.my_item {
    display: flex;
    flex-direction: row;
    position: relative;
    font-size: 30rpx;
    color: #222;
}

.order_icon {
    width: 14rpx;
    height: 25rpx;
    margin-right: 28rpx;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -8rpx!important;
}

.order_font {
    font-size: 32rpx;
    color: #222;
}

.code_mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    z-index: 1000;
}

.mob_code {
    z-index: 1001;
    background: #fff;
    position: fixed;
    top: 148rpx;
    left: 50%;
    margin-left: -280rpx;
    padding: 30rpx;
    width: 500rpx;
    height: 520rpx;
    border-radius: 10rpx;
    text-align: center;
}

.mob_code text {
    display: block;
}

.mob_code .title {
    font-size: 36rpx;
    color: #000;
    line-height: 36rpx;
    padding-top: 40rpx;
}

.mob_code .subTitle {
    width: 100%;
    font-size: 26rpx;
    line-height: 28rpx;
    padding-top: 10rpx;
    color: #888;
}

.mob_code .mobInput {
    text-align: left;
    border: 1rpx solid #e2e2e2;
    height: 88rpx;
    line-height: 88rpx;
    margin-top: 30rpx;
    padding-left: 30rpx;
    font-size: 30rpx;
    border-radius: 44rpx;
    background: #f5f5f5;
    color: #aaa;
}

.SMSverification {
    border: 1rpx solid #e2e2e2;
    margin-top: 30rpx;
    height: 88rpx;
    width: 100%;
    border-radius: 44rpx;
}

.SMSverification input {
    width: 260rpx;
    height: 88rpx;
    line-height: 88rpx;
    float: left;
    text-align: left;
    color: #555;
    font-size: 30rpx;
    padding-left: 30rpx;
}

.input_place_hoder {
    font-size: 30rpx;
    color: #aaa;
}

.SMSverification button {
    float: right;
    background: #fff;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 44rpx;
    width: 39%;
    font-size: 26rpx;
    margin: 0;
    padding: 0;
    color: #d3a95a;
}

.SMSverification button::after {
    border: 0;
}

.getCode[disabled]:not([type]) {
    background: #fff;
    color: #888;
}

.next {
    font-size: 34rpx;
    color: #fff;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 44rpx;
    background: #d3a95a;
    margin: 30rpx 0;
}

.next[disabled]:not([type]) {
    background: #d3a95a;
    color: #fff;
    opacity: 0.4;
}

.close_mask {
    position: absolute;
    height: 60rpx;
    width: 60rpx;
    left: 50%;
    bottom: -160rpx;
    margin-left: -30rpx;
}

.close_mask image {
    width: 100%;
    height: 100%;
}

.vip-more {
}

.vip-more .top {
    margin-top: 20rpx;
    background: #fff;
    padding-left: 30rpx;
}

.vip-more .top .item {
    border-bottom: 1rpx solid #e2e2e2;
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 30rpx;
}

.vip-more .top .noborder {
    border: none;
}

.vip-more .top .item .arrwo {
    width: 14rpx;
    height: 25rpx;
}

.vip-more .top .item .arrwo image {
    width: 14rpx;
    height: 25rpx;
    float: left;
}

.signOut {
    background: #fff;
    margin-top: 20rpx;
    text-align: center;
    color: #d3a95a;
    height: 90rpx;
    line-height: 90rpx;
}