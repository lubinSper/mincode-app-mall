page {
    background-color: #fff;
}

.intro_container {
    position: relative;
}

.intro_container view {
    line-height: 0rpx;
}

.intro1 image {
    width: 100%;
    height: 418rpx;
}

.intro2 {
    margin-top: 32rpx;
    padding: 0 28rpx;
}

.intro2 image {
    width: 100%;
    height: 410rpx;
}

.intro3 {
    margin-top: 32rpx;
}

.intro3 image {
    width: 100%;
    height: 624rpx;
}

.intro4 {
    margin-top: 40rpx;
    padding: 0 56rpx;
}

.intro4 image {
    width: 100%;
    height: 820rpx;
}

.intro5 {
    margin-top: 56rpx;
    margin-bottom: 52rpx;
    padding: 0 10rpx;
}

.intro5 image {
    display: block;
    width: 312rpx;
    height: 78rpx;
    margin: 0 auto;
}

.intro_icon {
    position: fixed;
    top: 200rpx;
    right: 30rpx;
    width: 80rpx;
    height: 320rpx;
    z-index: 9;
}

.intro_icon view {
    margin-top: 26rpx;
    width: 80rpx;
    height: 80rpx;
}

.intro_icon view image {
    width: 80rpx;
    height: 80rpx;
}

.sun_code_view {
    display: flex;
    flex-wrap: wrap;
    margin: 10rpx 0 20rpx 0;
}

.sun_code_item {
    width: 32.5%;
    height: 280rpx;
    text-align: center;
}

.sun_code_item image {
    width: 202rpx;
    height: 202rpx;
}

.sun_code_name {
    font-size: 26rpx;
    font-weight: 400;
    line-height: 26rpx;
    color: #434343;
}

.buy_view {
    width: 100%;
    padding-top: 60rpx;
    height: 800rpx;
    background: #f9f8f8;
}

.buy_title {
    margin-bottom: 50rpx;
}

.buy_title image {
    display: block;
    width: 266rpx;
    height: 38rpx;
    margin: 0 auto;
}

.buy_box {
    padding: 0 36rpx;
}

.buy_box .buy_warp {
    width: 100%;
    display: flex;
}

.buy_box .buy_item:first-child {
    margin-left: 0;
}

.buy_box .buy_item {
    position: relative;
    margin-left: 5%;
    width: 47.5%;
    height: 652rpx;
    background: #ffffff;
    border: 1rpx solid #bfbfbf;
    border-radius: 8rpx;
    color: #a58a7c;
    overflow: hidden;
}

.buy_box .yellow_color {
    color: #ff7800;
}

.buy_box .txt_box1 {
    height: 120rpx;
    border-bottom: 1rpx solid #bfbfbf;
}

.buy_box .txt_box1 .txt1 {
    display: block;
    margin-top: 30rpx;
    height: 46rpx;
    line-break: 46rpx;
    text-align: center;
    font-size: 30rpx;
    font-weight: 500;
}

.buy_box .txt_box1_in {
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
}

.buy_box .txt_box1_in .txt2 {
    font-size: 24rpx;
}

.buy_box .txt_box1_in .txt3 {
    font-size: 46rpx;
    font-weight: 500;
}

.buy_box .btn_box {
    border-bottom: 1rpx solid #bfbfbf;
    padding: 20rpx 0;
}

.buy_box .btn_box .buy-btn1 {
    display: block;
    width: 200rpx;
    height: 46rpx;
    line-height: 46rpx;
    margin: 0 auto;
    background: #a58a7c;
    color: #ffffff;
    text-align: center;
    border-radius: 46rpx;
    font-size: 26rpx;
    font-weight: 400;
}

.buy_box .yellow_color .buy-btn1 {
    background: #ff7800;
}

.buy_box .txt_box2 {
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #bfbfbf;
    text-align: center;
    color: #434343;
}

.buy_box .txt_box2 .txt4 {
    font-size: 26rpx;
    font-weight: 400;
}

.buy_box .txt_box3 {
    height: 270rpx;
    padding: 0rpx 20rpx;
}

.buy_box .txt_box3 .txt5 {
    display: block;
    margin-top: 40rpx;
    height: 30rpx;
    line-height: 30rpx;
    font-size: 28rpx;
    font-weight: 500;
    color: #434343;
    text-align: left;
    margin-left: 28rpx;
}

.yellow_color .txt_box3 .txt5 {
    color: #ff7800;
}

.buy_box .txt6_item {
    display: block;
    height: 30rpx;
    text-align: left;
    padding-bottom: 6rpx;
}

.buy_box .txt6_item .right-icon {
    display: inline-block;
    width: 18rpx;
    height: 12rpx;
    margin-top: 9rpx;
    margin-right: 10rpx;
}

.buy_box .txt6_item .txt6 {
    line-height: 30rpx;
    font-size: 23rpx;
    font-weight: 400;
    color: #434343;
}

.buy_botm_bg {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 12rpx;
    background: #a58a7c;
}

.yellow_color .buy_botm_bg {
    background: #ff7800;
}

.submit_view {
    width: 100%;
    height: 530rpx;
    padding-top: 40rpx;
    box-sizing: border-box;
    background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/shop-multi/intro_bg.jpg) no-repeat left top / 100% 530rpx;
}

.submit_title {
    margin-bottom: 45rpx;
}

.submit_title image {
    display: block;
    width: 228rpx;
    height: 38rpx;
    margin: 0 auto;
}

.submit_row {
    height: 67rpx;
    line-height: 67rpx;
    margin-top: 26rpx;
    margin-bottom: 26rpx;
    clear: both;
}

.submit_row .submit_desc {
    font-size: 32rpx;
    margin-left: 80rpx;
    float: left;
    width: 100rpx;
}

.submit_desc_r {
    margin-left: 30rpx;
}

.submit_row input {
    float: left;
    width: 420rpx;
    height: 67rpx;
    line-height: 67rpx;
    border: 1rpx solid #e5e5e5;
    background-color: #fff;
    font-size: 28rpx;
    margin-left: 30rpx;
    padding-left: 30rpx;
}

.submit_btn {
    margin: 42rpx 0 56rpx 31.5%;
    width: 262rpx;
    height: 74rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 34rpx;
    color: #fff;
    letter-spacing: 10rpx;
    background-color: #ff7800;
    border-radius: 8rpx;
}

.remindText {
    text-align: center;
    font-size: 24rpx;
    color: red;
    line-height: 20rpx;
    height: 20rpx;
}

.intro_qrcode {
    text-align: center;
    padding: 60rpx 0;
}

.qrcode_img {
    margin: 0 auto;
    width: 180rpx;
    height: 180rpx;
}

.qrcode_img image {
    width: 180rpx;
    height: 180rpx;
}

.qrcode_desc1 {
    font-size: 30rpx;
    font-weight: 400;
    color: #434343;
    margin-top: 20rpx;
}

.qrcode_desc2 {
    font-size: 24rpx;
    font-weight: 300;
    color: #434343;
    margin-top: 10rpx;
}

.to_top {
    position: fixed;
    bottom: 94rpx;
    right: 30rpx;
    width: 95rpx;
    height: 95rpx;
    background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/xcx/intro_to_top.png) left top / 95rpx 95rpx;
}

.new_bottom {
    bottom: 40rpx;
}