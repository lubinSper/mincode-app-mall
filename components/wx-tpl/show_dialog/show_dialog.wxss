.main {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 999999999999999999999999;
    padding: 0 30rpx;
}

.dialog {
    background: rgba(0,0,0,0.8);
    min-height: 70rpx;
    min-width: 360rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 8rpx;
    color: #fff;
    font-size: 30rpx;
    padding: 0 20rpx;
}