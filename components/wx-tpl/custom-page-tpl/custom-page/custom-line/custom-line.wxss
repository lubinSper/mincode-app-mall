.custom_line {
    width: 100%;
    background: #fff;
    display: flex;
    align-items: center;
    padding: 0 0rpx;
    margin: 10rpx 0;
}

.maxwidth {
    padding: 0 0rpx;
    margin: 0;
}

.custom_line .line {
    height: 0rpx;
    width: 100%;
}

.custom_line .solid {
    border-top: 1rpx solid #cecbcb;
}

.custom_line .dashed {
    border-top: 1rpx dashed #cecbcb;
}

.custom_line .dotted {
    border-top: 1rpx dotted #cecbcb;
}

.custom_line .colorBlock {
    border: 10rpx solid #f4f4f4;
}