.form {
    width: auto;
}

.form button {
    margin: 0;
    padding: 0;
}

.find_list_coupon {
    background-color: #fff;
    margin-top: 20rpx;
    position: relative;
}

.find_list_coupon .find_list_coupon_title {
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #222;
    text-indent: 30rpx;
    border-bottom: 0.5rpx solid #e5e5e5;
}

.find_list_coupon .find_list_coupon_title navigator {
    float: right;
    margin-right: 20rpx;
    font-size: 26rpx;
    color: #888;
}

.find_list_coupon .find_list_coupon_title navigator image {
    width: 12rpx;
    height: 22rpx;
}

.find_list_coupon .find_list_coupon_content {
    width: 100%;
    display: block;
    overflow: hidden;
    padding: 30rpx 30rpx 0;
}